{
  "name": "@ssv-labs/ssv-sdk",
  "version": "0.1.2",
  "author": "SSV.Labs",
  "description": "ssv labs sdk",
  "keywords": [
    "ssv",
    "sdk",
    "ssv-keys",
    "keyshares"
  ],
  "repository": "https://github.com/ssvlabs/ssv-sdk",
  "files": [
    "dist"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "vite build",
    "codegen": "graphql-codegen",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "v-pub-tarball": "npm publish --dry-run",
    "type-check": "tsc --noEmit",
    "type:perf": "tsc --extendedDiagnostics",
    "release": "release-it",
    "commit": "git-cz",
    "lint-staged": "pnpm run type-check && pnpm vitest run && lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,json}": [
      "prettier --write --log-level silent",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.14.0",
    "@chainsafe/bls-keystore": "3.1.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/schema-ast": "4.1.0",
    "@graphql-codegen/typed-document-node": "5.1.2",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-graphql-request": "6.3.0",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@nomicfoundation/hardhat-toolbox": "6.0.0",
    "@nomicfoundation/hardhat-viem": "2.0.6",
    "@openzeppelin/contracts": "4.9.6",
    "@openzeppelin/contracts-upgradeable": "4.9.6",
    "@openzeppelin/hardhat-upgrades": "3.9.1",
    "@parcel/watcher": "2.5.1",
    "@release-it/conventional-changelog": "8.0.2",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@tanstack/react-query": "^5.90.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/eslint-plugin-prettier": "3.1.3",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.19.7",
    "@types/node-forge": "1.3.13",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "7.7.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "1.6.1",
    "@vitest/ui": "1.6.1",
    "@zerodev/multi-chain-ecdsa-validator": "5.4.5",
    "@zerodev/sdk": "5.5.3",
    "commitizen": "4.3.1",
    "concurrently": "9.2.0",
    "cz-git": "1.12.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "hardhat": "2.25.0",
    "husky": "9.1.7",
    "jsdom": "24",
    "lint-staged": "15.5.2",
    "prettier": "3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "release-it": "17.11.0",
    "rollup": "4.45.0",
    "rollup-plugin-delete": "2.2.0",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-tsconfig-paths": "1.5.2",
    "rollup-plugin-typescript2": "0.36.0",
    "semantic-release": "23.1.1",
    "semver": "7.7.2",
    "typescript": "5.8.3",
    "vite": "5.4.19",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-glob": "0.3.2",
    "vite-plugin-node-polyfills": "0.24.0",
    "vitest": "1.6.1",
    "wagmi": "^2.18.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@safe-global/protocol-kit": "^5.2.10",
    "@wagmi/core": "^2.22.1",
    "abitype": "^1.0.6",
    "axios": "^1.12.2",
    "bls-eth-wasm": "^1.4.0",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.5",
    "graphql-request": "^7.2.0",
    "lodash-es": "^4.17.21",
    "node-forge": "^1.3.1",
    "scrypt-js": "^3.0.1",
    "tslib": "^2.8.1",
    "viem": "^2.38.4",
    "zod": "^3.23.8"
  },
  "license": "SEE LICENSE IN LICENCE FILE",
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.5",
    "react": "^18.0.0",
    "wagmi": "^2.18.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.26"
    }
  }
}
